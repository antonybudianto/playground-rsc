  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "1.19.1"
  },
  "scripts": {
    "gen": "node manifestGenerator.js",
    "clean": "rm -rf ./build",
    "dev": "npm run clean && concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cross-env NODE_ENV=development webpack --watch --config=./scripts/build.js",
    "dev:server": "cross-env NODE_ENV=development webpack --watch --config=./scripts/build-server.js",
    "gen:prod": "cross-env NODE_ENV=production CI=1 node manifestGenerator.js",
    "build": "npm run build:client && npm run gen:prod && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config=./scripts/build.js",
    "build:server": "cross-env NODE_ENV=production webpack --config=./scripts/build-server.js",
    "start": "cross-env NODE_ENV=production node build/server",
    "plugin": "NODE_ENV=development babel --extensions \\\".es6,.js,.es,.jsx,.mjs,.ts,.tsx\\\" --watch plugin/**.ts --out-dir ./plugin",
    "prettier": "prettier --write **/*.js"
  }
}
